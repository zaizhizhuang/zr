<launch>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node_com7" output="screen" respawn="true" >
        <param name="port" value="COM7" type="string" />
        <param name="baud" value="500000" type="int" />
        <remap from="led8_data8" to="zzz/led8"/>
        <remap from="zr_hw_info" to="zzzros/srv"/>
    </node>
    <node pkg="zr_led8_595" type="led8_pub.py" name="led8_pub_node" output="log" respawn="true" >
        <remap from="led8_data8" to="zzz/led8"/>
        <param name="rate_param" value="30" type="int" />
    </node>
     <node pkg="zr_hw_info" type="info_srv_client.py" name="infoclient" output="screen" respawn="true" >
        
        <param name="srv_param" value="zzzros/srv" type="string" />
        <remap from="zr_hw_info" to="zzzros/srv"/>
    </node>

</launch>